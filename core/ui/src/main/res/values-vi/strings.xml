<resources>
    <!--
      - Titles for the dialogs displayed as overlay.
      - Allowed characters can be small due to save/delete/close buttons.
      -->
    <string name="dialog_overlay_title_warning">Cảnh báo</string>
    <string name="dialog_overlay_title_copy_from">Sao chép từ</string>
    <string name="dialog_title_tutorial">Hướng dẫn</string>

    <!--
      - Text for items in a list or vertical layout.
      -  * Title should be short and concise.
      -  * Desc can have more space to describe the title
      -  * Error have the same space as desc
      -->
    <string name="item_title_click_position">Vị trí nhấn</string>
    <string name="item_title_swipe_positions">Vị trí vuốt</string>
    <string name="item_error_action_invalid_generic">Hành động này không hợp lệ</string>

    <!--
      - Empty list messages.
      - Views handles any length here, but try to be concise.
      -->
    <string name="message_empty_copy">Không có gì để sao chép</string>

    <!--
      - Messages.
      - Views handles any length here, so no limitations.
      -->
    <string name="message_tutorial_volume_down_stop">Nếu kịch bản của bạn nhấp chuột quá nhanh, nó có thể
        chặn bạn khỏi sử dụng menu Smart AutoClicker để dừng nó.\n\nTrong trường hợp đó, bạn có thể nhấn
        nút \"Giảm âm lượng\" trên thiết bị của mình để dừng kịch bản đang chạy.</string>


    <!--
      - Text input fields labels and error messages.
      - Should be short and concise.
      -->
    <string name="input_field_label_anti_detection">Chống phát hiện</string>
    <string name="input_field_label_click_press_duration">Thời gian nhấn (ms)</string>
    <string name="input_field_label_name">Tên</string>
    <string name="input_field_label_pause_duration">Thời gian tạm dừng (ms)</string>
    <string name="input_field_label_scenario_name">Tên kịch bản</string>
    <string name="input_field_label_swipe_duration">Thời gian vuốt (ms)</string>
    <string name="input_field_error_required">Bắt buộc</string>


    <!--
      - Dropdown input fields texts.
      - Those fields are made of:
      - * a label, describing the meaning of the field. Should be short and concise.
      - * a set of items, made of:
      -     * a title, displayed in the dropdown list and in the field if selected. Should be short and concise.
      -     * a helper text (if needed), displayed bellow the field and changed with the selected item. Max 2 lines.
      -->
    <!-- Dropdown field for selecting anti bot state -->
    <string name="dropdown_item_title_anti_detection_enabled">Bật</string>
    <string name="dropdown_item_title_anti_detection_disabled">Tắt</string>
    <string name="dropdown_helper_text_anti_detection_enabled">Ngẫu nhiên vị trí nhấn, vuốt và thời gian tạm dừng bằng một khoảng lùi ngẫu nhiên nhỏ</string>
    <string name="dropdown_helper_text_anti_detection_disabled">Thực hiện tất cả các hành động như đã được khai báo</string>

    <!--
      - Toast messages.
      - Can be pretty long (2 lines).
      -->
    <string name="toast_configure_single_click">Chạm vào vị trí để nhấn</string>
    <string name="toast_configure_swipe_from">Chạm vào vị trí để bắt đầu vuốt từ</string>
    <string name="toast_configure_swipe_to">Chạm vào vị trí để kết thúc vuốt tới</string>

    <!--
      - Content description texts.
      - They are use as accessibility texts for visually impaired people. They will not be displayed but spoken.
      -->
    <string name="content_desc_add_button">Nút để tạo một phần tử mới</string>
    <string name="content_desc_copy_button">Nút để sao chép từ một phần tử hiện có</string>
    <string name="content_desc_move_menu">Di chuyển menu nổi</string>
    <string name="content_desc_play_pause_scenario">Phát hoặc tạm dừng kịch bản</string>
    <string name="content_desc_stop_clicker">Dừng Smart AutoClicker</string>
    <string name="content_desc_confirm">Xác nhận</string>
    <string name="content_desc_go_back">Quay lại hộp thoại</string>
</resources>